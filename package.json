{
  "name": "fe-dev-tools",
  "displayName": "fe-dev-tools",
  "publisher": "2333Ge",
  "description": "fe-dev-tools",
  "version": "0.0.3",
  "repository": "https://github.com/2333Ge/fe-dev-tools",
  "icon": "source/logo.jpeg",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "keywords": [
    "fe",
    "fe dev",
    "fe dev tools",
    "openai"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "%extension-title%",
        "properties": {
          "fe-dev-tools.prompts": {
            "type": "object",
            "description": "%prompts-description%"
          },
          "fe-dev-tools.curPromptKey": {
            "type": "string",
            "description": "%cur-prompt-key-description%"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "fe-dev-tools.showTools",
        "title": "打开fe-dev-tools工具面板"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "./bin/build.sh"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "openai": "^3.2.1"
  }
}
